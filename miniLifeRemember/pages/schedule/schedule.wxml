<view>
  <view class="sub-title">今日日程</view>
  <wux-timeline position="alternate">
    <wux-timeline-item custom>
      <wux-icon slot="dot" type="ios-time" size="23" /><!--自定义节点-->
      <button bindtap='selectSchedule' data-content='2018'>2018-10-24 v1.0.0</button>
    </wux-timeline-item>
      
    <wux-timeline-item content="2018-10-24 v1.0.0" custom>
      <wux-icon slot="dot" type="ios-time" size="23" /><!--自定义节点-->
      <button bindtap='selectSchedule' data-content='2018'>2018-10-24 v1.0.0</button>
    </wux-timeline-item>

    <wux-timeline-item content="2018-10-24 v1.0.0" custom>
      <wux-icon slot="dot" type="ios-time" size="23" /><!--自定义节点-->
    </wux-timeline-item>

    <wux-timeline-item  custom>
      <wux-icon slot='dot' type='ios-time' size='23' />
      <wux-wing-blank size="default">
            <wux-card title="卡片标题" extra="额外内容" thumb="https://wux.cdn.cloverstd.com/logo.png">
                <view slot="body">卡片内容</view>
                <view slot="footer">尾部内容</view>
            </wux-card>
        </wux-wing-blank>
    </wux-timeline-item>

    <wux-timeline-item custom>
                <wux-icon slot="dot" type="ios-time" size="23" /><!--自定义节点-->
                <view>2018-10-24 v1.0.0</view>
            </wux-timeline-item>
  </wux-timeline>
